extends layout

block content

    #tweetContainer
        h2 Active Tweets
        ul#tweets
            each tweet in tweets
                li.cf
                    img(src='#{tweet.user.profile_image_url}')
                    p= tweet.user.name + " - " + tweet.text

    #tagCloud
        h2 Popular Hashtags
        ul#tagList
    #charts
        h2 Last 10 Minutes
        #realtime.chart
        h2 Last 7 Days
        #ajax.chart

    script(src="/js/flot/jquery.flot.js")
    script(src='/js/index.js')
